{
  "meta": {
    "version": "v1.1-nodeb",
    "description": "Node B optimized deterministic literature discovery queries for GaN CMOS demos & monolithic GaN IC platforms",
    "year_min": 2010
  },
  "buckets": [
    {
      "bucket_id": "NODE_B2_GaN_CMOS_Logic_Demos",
      "description": "True complementary CMOS logic demos: inverter / logic gate / ring oscillator; includes key metric terms",
      "queries": [
        "\"GaN CMOS\" inverter",
        "\"complementary\" GaN inverter",
        "\"p-channel\" GaN inverter",
        "\"GaN\" \"ring oscillator\"",
        "\"GaN\" \"logic gate\"",
        "\"GaN\" \"voltage transfer\" inverter",
        "\"GaN\" \"noise margin\" inverter",
        "\"all-GaN\" CMOS inverter",
        "\"monolithic\" GaN CMOS"
      ]
    },
    {
      "bucket_id": "NODE_B1_Monolithic_GaN_IC_Platforms",
      "description": "Monolithic GaN IC platform/enablers: GaN-on-SOI, deep trench isolation, integrated GaN power IC components, logic on GaN",
      "queries": [
        "\"GaN-on-SOI\" integrated circuit",
        "\"GaN-on-SOI\" monolithically integrated",
        "\"monolithic\" GaN integrated circuit",
        "\"all-GaN\" integrated circuit",
        "\"GaN power IC\" monolithic",
        "\"deep trench\" GaN isolation",
        "\"GaN\" \"power integrated circuit\"",
        "\"GaN\" \"integrated driver\" IC",
        "\"GaN\" \"monolithic integration\""
      ]
    },
    {
      "bucket_id": "NODE_B3_Near_CMOS_Context",
      "description": "Near-CMOS / non-true-CMOS logic context (useful for motivation but should not be mistaken as B2)",
      "queries": [
        "\"GaN\" \"depletion load\" inverter",
        "\"GaN\" \"resistive load\" inverter",
        "\"GaN\" pseudo-CMOS inverter",
        "\"GaN\" RTL inverter",
        "\"GaN\" logic inverter depletion"
      ]
    }
  ]
}
